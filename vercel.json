{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "env": {
    "NODE_ENV": "production",
    "POSTGRES_STATEMENT_TIMEOUT": "60000",
    "PRISMA_CLIENT_ENGINE_TYPE": "binary",
